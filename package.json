{
  "name": "player",
  "version": "1.0.0",
  "description": "javascript로 만든mp3플레이어",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode=development",
    "prod": "webpack --mode=production",
    "watch": "webpack --watch --progress",
    "start": "electron .",
    "x64build": "electron-packager . jsplayer --asar --platform=win32 --arch=x64 --out=dist64 --overwrite",
    "x32build": "electron-packager . jsplayer --asar --platform=win32 --arch=ia32 --out=dist32 --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gondr99/jsplayer.git"
  },
  "keywords": [
    "javascript",
    "player",
    "audio",
    "visualizer"
  ],
  "author": "gondr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gondr99/jsplayer/issues"
  },
  "homepage": "https://github.com/gondr99/jsplayer#readme",
  "dependencies": {
    "fs": "0.0.1-security",
    "jsmediatags": "^3.9.2"
  },
  "devDependencies": {
    "asar": "^2.0.1",
    "css-loader": "^3.2.0",
    "electron": "^6.0.12",
    "electron-packager": "^14.0.6",
    "mini-css-extract-plugin": "^0.8.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
